<h4>Archives</h4>
<hr>
<% @posts_by_month.each do |monthname, posts| %>
<p class="month-archive"><%= monthname %></p>
 <div class="archive">
    <ul class="archive">
    <% posts.each do |post| %>
    <li class="archive"><p class="archive-post"><%=link_to post.title, post %></p></li>
    <% end %>
    </ul>
</div>
<%end%>